{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "phone": {
      "type": "string"
    },
    "address": {
      "type": "text"
    },
    "city": {
      "type": "string"
    },
    "state": {
      "type": "string"
    },
    "zipcode": {
      "type": "string"
    },
    "TotalOrderedAmount": {
      "type": "decimal"
    },
    "userId": {
      "type": "integer"
    },
    "paymentId": {
      "type": "string"
    },
    "orderItemList": {
      "type": "component",
      "repeatable": true,
      "component": "ordered-item.ordered-item"
    },
    "Status": {
      "type": "enumeration",
      "enum": [
        "Pending",
        "Shipping",
        "Out for Delivery",
        "Delivered"
      ],
      "default": "Pending"
    }
  }
}
